import seaborn as sns
import matplotlib.pyplot as plt
import pandas as pd
import numpy as np
import csv
import fractions

#establish heatmap of 2022
from fractions import Fraction as F
numbers = {}

#extract row_1 
with open(r"C:\Users\user\Desktop\baseballdata\data2022_edited_FF.csv", mode="r", encoding="latin-1") as inp:
    reader = csv.reader(inp)
    # Skips the heading  
    heading = next(inp) 
    numbers = {rows[0]: rows[1] for rows in reader}
#extract row_3
with open(r"C:\Users\user\Desktop\baseballdata\data2022_edited_FF.csv", mode="r", encoding="latin-1") as inp:
    reader = csv.reader(inp)
    # Skips the heading  
    heading = next(inp) 
    ratios = {rows[0]: rows[3] for rows in reader}

# Convert string keys to integers
numbers_int = dict((int(k), int(v)) for k,v in numbers.items())
ratios_result = dict((int(k), float(v)) for k,v in ratios.items())



# Create an initial 8x8 matrix
# Create an initial 8x8 matrix
strikezone_1 = [[11, 11, 11, 11, 12, 12, 12, 12],
    [11, 1, 1, 2, 2, 3, 3, 12],
    [11, 1, 1, 2, 2, 3, 3, 12],
    [11, 4, 4, 5, 5, 6, 6, 12],
    [13, 4, 4, 5, 5, 6, 6, 14],
    [13, 7, 7, 8, 8, 9, 9, 14],
    [13, 7, 7, 8, 8, 9, 9, 14],
    [13, 13, 13, 13, 14, 14, 14, 14]]
strikezone_2 = [[11, 11, 11, 11, 12, 12, 12, 12],
    [11, 1, 1, 2, 2, 3, 3, 12],
    [11, 1, 1, 2, 2, 3, 3, 12],
    [11, 4, 4, 5, 5, 6, 6, 12],
    [13, 4, 4, 5, 5, 6, 6, 14],
    [13, 7, 7, 8, 8, 9, 9, 14],
    [13, 7, 7, 8, 8, 9, 9, 14],
    [13, 13, 13, 13, 14, 14, 14, 14]]

numbers_corres_lst = strikezone_1.copy()
ratio_corres_lst = strikezone_2.copy()


#convert strikezone to numbers
for i in range(8):
    for j in range(8):
        numbers_corres_lst[i][j] = numbers_int[strikezone_1[i][j]]

#convert strikezone to ratio
for i in range(8):
    for j in range(8):
        ratio_corres_lst[i][j] = ratios_result[strikezone_2[i][j]]

#將ratio_corres_lst轉成ndarray再reshape 8*8 array
ratio_corres_lst = np.reshape(np.c_[ratio_corres_lst],(8,8))
#將numbers_corres_lst轉成ndarray再reshape 8*8 array
numbers_corres_lst = np.reshape(np.c_[numbers_corres_lst],(8,8))


formatted_text = (np.asarray(["{0:.2f}\n ({1})".format( 
    ratio_corres_lst, numbers_corres_lst) for ratio_corres_lst, numbers_corres_lst in zip(ratio_corres_lst.flatten(), numbers_corres_lst.flatten())])).reshape(8, 8) 


# drawing heatmap 
fig, ax = plt.subplots() 
ax = sns.heatmap(ratio_corres_lst, annot=formatted_text, fmt="", cmap="Blues", vmax=1, vmin=0)
ax.set_title('2022 ball of no swing') 

